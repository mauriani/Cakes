n      {totalItems === false ? (\r\n        <Container>\r\n          <span>\r\n            Até o momento você não adicionou nenhum item no carrinho !\r\n            <div>\r\n              <MdRemoveShoppingCart size={35} color=\"#7159c1\" />\r\n            </div>\r\n          </span>\r\n        </Container>\r\n      ) : (\r\n        <Container>\r\n          <ProductTable>\r\n            <table>\r\n              <thead>\r\n                <tr>\r\n                  <th>Produto</th>\r\n                  <th>Preço</th>\r\n                  <th>Quantidade</th>\r\n                  <th>Subtotal</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                {cart.items.map((item) => (\r\n                  <tr key={item.product.id}>\r\n                    <td>\r\n                      <img src={item.product.image} alt={item.product.title} />\r\n                    </td>\r\n\r\n                    <td>\r\n                      <strong>{item.product.title}</strong>\r\n                      <span>{item.product.priceFormatted}</span>\r\n                    </td>\r\n\r\n                    <td>\r\n                      <div>\r\n                        <button\r\n                          type=\"button\"\r\n                          onClick={() => decrement(item.product, item.quantity)}\r\n                        >\r\n                          <MdRemoveCircleOutline size={20} color=\"#7159c1\" />\r\n                        </button>\r\n\r\n                        <input type=\"number\" readOnly value={item.quantity} />\r\n\r\n                        <button\r\n                          type=\"button\"\r\n                          onClick={() => increment(item.product, item.quantity)}\r\n                        >\r\n                          <MdAddCircleOutline size={20} color=\"#7159c1\" />\r\n                        </button>\r\n                      </div>\r\n                    </td>\r\n\r\n                    <td>\r\n                      <